    "url": "https://github.com/1onlinesolution/dws-express-app/issues"
  },
  "homepage": "https://github.com/1onlinesolution/dws-express-app#readme",
  "devDependencies": {
    "dotenv": "^10.0.0",
    "eslint": "^7.28.0",
    "eslint-plugin-security": "^1.4.0",
    "mocha": "^9.0.0",
    "prettier": "^2.3.1"
  },
  "dependencies": {
    "@1onlinesolution/dws-http": "^0.0.3",
    "@1onlinesolution/dws-session": "^0.0.2",
    "@1onlinesolution/dws-utils": "^0.0.2",
